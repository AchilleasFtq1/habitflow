{
  "expo": {
    "name": "habitflow",
    "slug": "habitflowv2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/habitflow_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/habitflow_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "cy.com.habitflow",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/habitflow_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/habitflow_icon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2a8198f4-97c4-4323-ab2f-e1f4930c2acd"
      }
    },
    "owner": "aeftychiou"
  }
}
